<?xml version="1.0" encoding="utf-8"?>
<package >
  <metadata>
    <id>Kanvas</id>
    <version>2.0.8</version>
    <description>A library containing image transcodings and quantizations usable in the Kuriimu2 eco-system.</description>

    <authors>onepiecefreak;IcySon55</authors>
    <owners>onepiecefreak;IcySon55</owners>
    <copyright>Copyright (c) 2019 Fan Translators International</copyright>

    <projectUrl>https://github.com/FanTranslatorsInternational/Kuriimu2</projectUrl>
    <icon>kuriimu2icon.png</icon>
    <tags>Kuriimu2;Image;Transcoding;Quantization</tags>

    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <license type="expression">GPL-3.0-only</license>

    <dependencies>
      <group targetFramework="net5.0">
        <dependency id="BCnEncoder.Net" version="2.0.1" />
        <dependency id="System.Drawing.Common" version="5.0.0" />
      </group>
    </dependencies>
  </metadata>

  <files>
    <file src="bin\Debug\net5.0\*.dll" target="lib\net5.0\"/>
    <file src="lib\win-x64\*.dll" target="runtimes\win-x64\native\"/>
    <file src="lib\win-x32\*.dll" target="runtimes\win-x32\native\"/>
    <file src="lib\linux-x64\*.so" target="runtimes\linux-x64\native\"/>
    <file src="lib\linux-x32\*.so" target="runtimes\linux-x32\native\"/>
    <file src="lib\osx-x64\*.dylib" target="runtimes\osx-x64\native\"/>
    <file src="..\..\..\kuriimu2icon.png" target="kuriimu2icon.png"/>
  </files>
</package>